msgstr ""
"  \"/msg username [text]\" - Отправить личное сообщение с текстом "
"пользователю username."

#: src/gui/ui.cpp:254
msgid "  \"/part\" - Leaves current channel."
msgstr "  \"/part\" - Покинуть текущий канал."

#: src/gui/ui.cpp:255
msgid "  \"/p\" - Alias to /part."
msgstr "  \"/p\" - Псевдоним /part."

#: src/gui/ui.cpp:256
